def perfect(n):
  sum=0
  for i in range(1,n):
      if n%i==0:
          sum=sum+1
  if sum==n:
      return True
  else:
       return False
n=int(input())
if 1<=n<=100000:
 if perfect(n):
    print('Perfect Number')
 else:
    print('Not Perfect Number')
else:
    print('Input number is out of range')
